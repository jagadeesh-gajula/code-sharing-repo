    "import matplotlib.pyplot as plt\n",
    "import numpy as np\n",
    "\n",
    "def sig(x):\n",
    "    return 1 / (1 + np.exp(x))\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 36,
   "metadata": {},
   "outputs": [],
   "source": [
    "x = np.linspace(-10,10,10000)\n",
    "sig_x = sig(x)"
   ]
